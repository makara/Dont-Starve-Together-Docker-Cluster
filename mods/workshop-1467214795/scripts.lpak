                if data and not data.rescueitem then
                    data.rescueitem = inst -- require a rescueitem for logic
                end
                inst.sg:GoToState("death_boat", data)
            elseif IsOnLand(inst) then
                if data and not data.rescueitem then
                    data.rescueitem = inst -- require a rescueitem for logic
                end
                --inst.sg:GoToState("death_boat", data)
            end
        end          
    end),

    EventHandler("vacuum_in", function(inst)
