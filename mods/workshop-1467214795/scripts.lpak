{
    ["small"] = {duration = 0.7/2, speed = 0.02, scale = 0.75/2},
    ["med"] = {duration = 0.7, speed = 0.02, scale = 0.75},
    ["large"] = {duration = 2*0.7, speed = 0.02, scale = 2*0.75},
