	inst:DoTaskInTime(1, function()
		inst.components.floodable:SetFX(nil,10) --now update normal again
	end)

  inst.entity:SetPristine()

  if not TheWorld.ismastersim then
    return inst
  end

  inst:AddComponent("inspectable")
