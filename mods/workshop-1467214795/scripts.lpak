    local function onhit(inst)
		if data.destroysound ~= nil then
			inst.SoundEmitter:PlaySound(data.destroysound)
		elseif data.material ~= nil then
            inst.SoundEmitter:PlaySound("dontstarve/common/destroy_"..data.material)
        end

