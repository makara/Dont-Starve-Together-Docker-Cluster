				idx = i
			end
		end
	end

	--print("get_angle2 return", dirs[idx].rads / DEGREES, math.atan2(dirs[idx].z, dirs[idx].x) / DEGREES)
	return dirs[idx].rads / DEGREES --math.atan2(dirs[idx].z, dirs[idx].x) / DEGREES
