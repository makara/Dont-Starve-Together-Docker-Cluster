	if mommy and mommy.components.mateable:CanMate() and mommy.sg
	and daddy and daddy.components.mateable:CanMate() and daddy.sg
	and mommy:GetDistSqToInst(daddy) <= TUNING.DOYDOY_MATING_RANGE then
