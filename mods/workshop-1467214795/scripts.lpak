        if stage == 1 then
            inst.components.pickable:MakeBarren()
        end

        inst:AddComponent("lootdropper")
        inst:AddComponent("inspectable")
		
        ---------------------

        -- MakeMediumBurnable(inst)
        -- MakeSmallPropagator(inst)
