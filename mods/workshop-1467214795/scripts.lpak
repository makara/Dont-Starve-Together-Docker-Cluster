    local result_offset = FindValidPositionByFan(theta, radius, 12, function(offset)
        local spawn_point = pt + offset
        if GetGroundTypeAtPosition(spawn_point) == GROUND.OCEAN_SHALLOW then
            return true
        end
        return false
