		MakePickableBlowInWindGust(inst, TUNING.GRASS_WINDBLOWN_SPEED, TUNING.GRASS_WINDBLOWN_FALL_CHANCE)
       
	   ---------------------

        inst.OnPreLoad = OnPreLoad
        -- inst.MakeDiseaseable = makediseaseable

        return inst
    end

    return Prefab(name, fn, assets, prefabs)
