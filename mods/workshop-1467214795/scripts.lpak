    MakeInvItemIA(inst)
    inst.components.inventoryitem.nobounce = true
    inst.components.inventoryitem.canbepickedup = false
    inst.components.inventoryitem.canbepickedupalive = true
	inst.components.inventoryitem:SetSinks(false)
