----------------------------------------------------------------------------------------

IAENV.AddComponentPostInit("uianim", function(cmp)


cmp.Shake = Shake
old_OnWallUpdate = cmp.OnWallUpdate
cmp.OnWallUpdate = OnWallUpdate


end)
