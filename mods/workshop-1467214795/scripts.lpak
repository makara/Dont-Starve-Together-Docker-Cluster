-- end

-- function self:OnLoad(data)
    -- ToggleUpdate(true)
-- end

function self:OnPostInit()
    ToggleUpdate(true)
end

--------------------------------------------------------------------------
--[[ Debug ]]
--------------------------------------------------------------------------

function self:GetDebugString()
    return string.format("HailRain: updating:%s", tostring(_updating))
end

--------------------------------------------------------------------------
--[[ End ]]
--------------------------------------------------------------------------

end)
