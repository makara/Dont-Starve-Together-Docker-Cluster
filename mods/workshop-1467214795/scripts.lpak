	if equipper then
		equipper.AnimState:OverrideSymbol("swap_object", "swap_"..self.tool_type.."_obsidian", "swap_"..self.tool_type..suffix)
	end

	if self.onchargedelta then
		self.onchargedelta(self.inst, old, new)
	end
end

