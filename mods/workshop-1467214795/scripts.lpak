  local dist = TUNING.SNAKE_TARGET_DIST
  local notags = {"FX", "NOCLICK","INLIMBO", "wall", "snake", "structure", "aquatic"}
  return FindEntity(inst, dist, function(guy)
      return  inst.components.combat:CanTarget(guy)
    end, nil, notags)
